{
  "name": "local-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/local-provider.mjs",
  "exports": {
    ".": "./src/local-provider.mjs"
  },
  "description": "repository provider using local (native) git commands",
  "keywords": [
    "git",
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/local-provider.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/local-provider.mjs"
  },
  "dependencies": {
    "content-entry-filesystem": "^2.0.2",
    "execa": "^5.0.0",
    "globby": "^11.0.1",
    "one-time-execution-method": "^2.0.3",
    "repository-provider": "^24.8.6"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "repository-provider-test-support": "^1.2.31",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/local-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/local-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/local-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
