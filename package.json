{
  "name": "local-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/local-provider.mjs",
  "description": "repository provider using local (native) git commands",
  "keywords": [
    "git",
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/local-provider.mjs --section=API",
    "lint": "documentation lint src/local-provider.mjs"
  },
  "dependencies": {
    "content-entry": "^2.3.4",
    "execa": "^4.0.0",
    "globby": "^11.0.0",
    "one-time-execution-method": "^2.0.1",
    "repository-provider": "^17.0.1"
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "c8": "^7.1.1",
    "documentation": "^12.3.0",
    "repository-provider-test-support": "github:arlac77/repository-provider-test-support",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/local-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/local-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/local-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
