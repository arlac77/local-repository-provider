{
  "name": "local-repository-provider",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/local-provider.mjs",
  "module": "src/local-provider.mjs",
  "description": "repository provider using local (native) git commands",
  "keywords": [
    "git",
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/local-provider.mjs --section=API",
    "lint": "documentation lint src/local-provider.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "content-entry": "^2.2.2",
    "execa": "^2.0.4",
    "globby": "^10.0.1",
    "repository-provider": "^16.7.2"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "c8": "^5.0.2",
    "documentation": "^12.1.1",
    "esm": "^3.2.25",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.24"
  },
  "engines": {
    "node": ">=12.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/local-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/local-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/local-repository-provider#readme",
  "release": {},
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
