{
  "name": "local-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/local-provider.js",
  "module": "src/local-provider.js",
  "description": "repository provider using local (native) git commands",
  "keywords": [
    "git",
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage && nyc --temp-directory build/nyc ava",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/local-provider.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/local-provider.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "globby": "^8.0.1",
    "make-dir": "^1.3.0",
    "repository-provider": "^7.0.0",
    "tempy": "^0.2.1"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.1",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.10.8",
    "travis-deploy-once": "^5.0.9",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/local-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/local-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/local-repository-provider#readme",
  "release": {},
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
