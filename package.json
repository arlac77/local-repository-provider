{
  "name": "local-repository-provider",
  "version": "0.0.0-semantic-release",
  "main": "dist/local-repository-provider.js",
  "module": "src/local-repository-provider.js",
  "description": "repository provider using local (native) git commands",
  "keywords": [
    "git"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/local-repository-provider.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/local-repository-provider.js"
  },
  "dependencies": {
    "execa": "^0.8.0",
    "globby": "^7.1.1",
    "make-dir": "^1.1.0",
    "repository-provider": "^2.0.2",
    "tempy": "^0.2.1"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.1",
    "rollup": "^0.53.3",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^11.1.0",
    "documentation": "^5.3.5"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/local-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/local-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/local-repository-provider#readme",
  "release": {},
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
